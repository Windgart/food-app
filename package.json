{
  "name": "food-page",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "coverage": "vitest run --coverage",
    "dev": "vite",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json}  ./.prettierrc",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
    "preview": "vite preview",
    "prepare": "husky install",
    "test": "vitest --run",
    "type-check": "tsc --project tsconfig.json --pretty"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@mantine/carousel": "^5.9.4",
    "@mantine/core": "^5.9.4",
    "@mantine/hooks": "^5.9.4",
    "@tabler/icons": "^1.117.0",
    "embla-carousel-react": "^7.0.5",
    "miragejs": "^0.1.46",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@rollup/plugin-url": "^8.0.1",
    "@testing-library/react": "^13.4.0",
    "@types/node": "^18.11.15",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/coverage-c8": "^0.25.8",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "jsdom": "^20.0.3",
    "path": "^0.12.7",
    "prettier": "^2.8.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-tsconfig-paths": "^4.0.2",
    "vitest": "^0.25.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm format && pnpm lint && pnpm type-check && pnpm test"
    }
  }
}
